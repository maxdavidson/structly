{
  "name": "structly",
  "version": "0.4.1",
  "license": "MIT",
  "description": "Tool for working with binary data types in JavaScript",
  "author": "Max Davidson <davidsonmax@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/maxdavidson/structly.git"
  },
  "engineStrict": true,
  "engines": {
    "node": ">=4.6.0",
    "npm": ">=2.15.10"
  },
  "main": "dist/structly.umd.js",
  "module": "dist/index.js",
  "jsnext:main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "build": "npm run build:tsc && npm run build:rollup",
    "build:tsc": "tsc",
    "build:rollup": "rollup --config",
    "clean": "rimraf dist coverage .nyc_output",
    "lint": "tslint --project tsconfig.json --format stylish",
    "test": "nyc --cache ava",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "prebuild:tsc": "npm run clean",
    "pretest": "npm run build:tsc -- --module commonjs",
    "posttest": "npm run lint",
    "preversion": "npm test",
    "prepublish": "npm run build",
    "postversion": "git push && git push --tags && npm publish"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "binary",
    "pod",
    "struct",
    "record",
    "serialize",
    "encode",
    "decode",
    "convert",
    "pack",
    "unpack"
  ],
  "dependencies": {
    "@types/node": "^6.0.38"
  },
  "devDependencies": {
    "ava": "^0.16.0",
    "coveralls": "^2.11.14",
    "nyc": "^8.3.0",
    "rimraf": "^2.5.4",
    "rollup": "^0.36.0",
    "rollup-plugin-commonjs": "^5.0.4",
    "rollup-plugin-node-builtins": "^2.0.0",
    "rollup-plugin-node-globals": "^1.0.9",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-plugin-sourcemaps": "^0.4.1",
    "rollup-plugin-uglify": "^1.0.1",
    "tslint": "^3.15.1",
    "typescript": "^2.0.3"
  }
}
