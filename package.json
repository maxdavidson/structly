{
  "name": "structly",
  "version": "0.3.3",
  "license": "MIT",
  "description": "Tool for working with binary data types in JavaScript",
  "author": "Max Davidson <davidsonmax@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/maxdavidson/structly.git"
  },
  "engineStrict": true,
  "engines": {
    "node": ">=4.5.0",
    "npm": ">=2.15.9"
  },
  "main": "dist/structly.js",
  "module": "dist/structly.es.js",
  "jsnext:main": "dist/structly.es.js",
  "typings": "typings/index.d.ts",
  "scripts": {
    "build": "tsc && rollup -c",
    "clean": "rimraf dist js typings coverage .nyc_output",
    "lint": "tslint --project tsconfig.json --format stylish",
    "test": "tsc -m commonjs && nyc ava",
    "test:watch": "tsc -m commonjs && concurrently -kr \"ava -w\" \"tsc -w -m commonjs\"",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "pretest": "npm run clean",
    "posttest": "npm run lint",
    "preversion": "npm test",
    "prepublish": "npm run build",
    "postversion": "git push && git push --tags && npm publish"
  },
  "files": [
    "dist",
    "typings"
  ],
  "keywords": [
    "binary",
    "pod",
    "struct",
    "record",
    "serialize",
    "encode",
    "decode",
    "convert",
    "pack",
    "unpack"
  ],
  "dependencies": {
    "@types/node": "^6.0.38"
  },
  "devDependencies": {
    "ava": "^0.16.0",
    "concurrently": "^2.2.0",
    "coveralls": "^2.11.14",
    "nyc": "^8.3.0",
    "rimraf": "^2.5.4",
    "rollup": "^0.36.0",
    "rollup-plugin-sourcemaps": "^0.4.1",
    "tslint": "^3.15.1",
    "typescript": "^2.0.3"
  }
}
