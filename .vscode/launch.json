{
  "version": "0.2.0",
  "configurations": [{
    "name": "Debug tests",
    "type": "node",
    "request": "launch",
    "protocol": "inspector",
    "stopOnEntry": false,
    "args": [
      "node_modules/.bin/jest",
      "--no-cache",
      "--runInBand",
      "--env=node-debug"
    ],
    "skipFiles": [
      "<node_internals>/**/*.js",
      "${workspaceRoot}/node_modules/**/*.js"
    ]
  }]
}
